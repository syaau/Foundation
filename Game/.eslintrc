{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "import/no-extraneous-dependencies": ["error", { "peerDependencies": true }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"]}]
  },
  "settings": {
    "import/core-modules": [
    ]
  },
  "globals": {
    "__DEV__": true
  }
}